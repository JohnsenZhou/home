<div class="demo-content">
  {{#each githubList}}
    <div class="demo-item" data-url={{url}}>
      <p class="demo-title">{{name}}</p>
      <div class="demo-icon">
        {{!-- <iframe src="https://ghbtns.com/github-btn.html?user=johnsenzhou&repo={{name}}&type=star&count=true" frameborder="0" scrolling="0" width="104px" height="20px"></iframe> --}}
        {{!-- <iframe src="https://ghbtns.com/github-btn.html?user=johnsenzhou&repo={{name}}&type=fork&count=true" frameborder="0" scrolling="0" width="104px" height="20px"></iframe> --}}
        <div class="git-detail"><span class="git-star">Star</span></div>
        <div class="number"><span class="">{{stars}}</span></div>
        <div class="git-detail"><span class="git-fork">Fork</span></div>
        <div class="number"><span class="">{{forks}}</span></div>
      </div>
      <p>{{description}}</p>
    </div>
  {{/each}}
</div>